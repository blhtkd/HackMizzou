DROP SCHEMA IF EXISTS Guidance CASCADE;
CREATE SCHEMA Guidance;

SET search_path = Guidance, public;

DROP TABLE IF EXISTS CS_Courses;
CREATE TABLE CS_Courses (
	course_name_num char(7) NOT NULL default '',
	title varchar(100) NOT NULL default '',
	prereq varchar(10) NOT NULL default '',
	credits varchar(10) NOT NULL default '0',
	taken NUMERIC(1) NOT NULL default '0',
	PRIMARY KEY (course_name_num)
);

--INSERT INTO CS_Courses values ('CS 1000', 'Intro to Comp Sci', '', '1');

DROP TABLE IF EXISTS Math_Courses;
CREATE TABLE Math_Courses (
	course_name_num varchar(20) NOT NULL default '',
	title varchar(100) NOT NULL default '',
	prereq varchar(20) NOT NULL default '',
	credits varchar(10) NOT NULL default '0',
	taken NUMERIC(1) NOT NULL default '0',
	PRIMARY KEY (course_name_num)
);
DROP TABLE IF EXISTS Stat_Courses;
CREATE TABLE Stat_Courses (
	course_name_num varchar(20) NOT NULL default '',
	title varchar(100) NOT NULL default '',
	prereq varchar(20) NOT NULL default '',
	credits varchar(10) NOT NULL default '0',
	taken NUMERIC(1) NOT NULL default '0',
	PRIMARY KEY (course_name_num)
);

--INSERT INTO Math_Courses values ('MATH 1500', 'Calc 1', '', '5');

DROP TABLE IF EXISTS Science_Courses;
CREATE TABLE Science_Courses (
	course_name_num varchar(20) NOT NULL default '',
	title varchar(100) NOT NULL default '',
	prereq varchar(100) NOT NULL default '',
	credits varchar(10) NOT NULL default '0',
	taken NUMERIC(1) NOT NULL default '0',
	PRIMARY KEY (course_name_num)
);

